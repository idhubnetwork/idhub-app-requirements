{
  "name": "idhub-app-requirements",
  "version": "0.0.1",
  "description": "documentation",
  "repository": {
    "url": "https://github.com/idhubnetwork/idhub-app-requirements.git",
    "type": "git"
  },
  "main": "dist/doc.js",
  "module": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint . --ext .js --ext .vue",
    "website": "poi --config website/poi.config.js",
    "build:website": "poi build --config website/poi.config.js",
    "build": "poi build --config build/poi.config.js",
    "build:lib": "sfc normalize src -d lib --include \"**/*.*\" --include \"!**/__test__/**\" --no-babelrc",
    "prepublishOnly": "npm run build:lib && npm run build",
    "pregh": "poi build --config website/poi.config.js",
    "gh": "gh-pages -d dist"
  },
  "author": "jj <jerry7722@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "jump.js": "^1.0.2",
    "loadjs": "^3.5.4",
    "markdown-it": "^8.4.1",
    "medium-zoom": "^0.4.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.8.4",
    "vue": "^2.5.16",
    "vue-content-loader": "^0.2.1",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-config-xo": "^0.22.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.5.0",
    "gh-pages": "^1.2.0",
    "husky": "^1.0.0-rc.8",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "poi": "^10.2.7",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.13.5",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.11.1",
    "sfc": "^0.2.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint . --fix",
      "git add"
    ]
  }
}
